{
    "version": 2,
    "builds": [
        { "src": "/frontend/index.html", "use": "@vercel/static" },
        {
        "src": "/index.js",
        "use": "@vercel/node"
        }
    ],
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/index.js" },
        { "source": "(.*)", "destination": "/frontend/index.html" }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/index.js"
        },
        {
          "src": "/(.*)",
          "dest": "/frontend/index.html"
        }
      ]
}