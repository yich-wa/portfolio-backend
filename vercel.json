{
    "version": 2,
    "builds": [
        { "src": "/index.js", "use": "@vercel/node" },
        { "src": "/frontend/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
           "src": "/api/(.*)",
           "dest": "/index.js",
           "continue": false
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
      ]
}